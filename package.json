{
  "name": "WINSMB",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "test": "NODE_ENV=test jest",
    "ios": "react-native run-ios",
    "ios-device": "react-native run-ios --device",
    "ios-release": "react-native run-ios --device --configuration Release",
    "android_old": "react-native run-android",
    "android": "cd ./android && ./gradlew app:assembleDebug && ./gradlew installDebug",
    "android-release": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ../",
    "clean": "watchman watch-del-all && rm -fr $TMPDIR/react* && npm cache clean --force && rm -rf node_modules && yarn install && cd android && ./gradlew clean",
    "clean-win": "rimraf node_modules  && npm install && adb kill-server && adb devices && adb reverse tcp:8081 tcp:8081 && rimraf ./android/app/build",
    "adb-restart": "adb kill-server && adb start-server && adb devices && adb reverse tcp:8081 tcp:8081",
    "ios-clean": "npm run clean && npm run ios",
    "dashboard": "parse-dashboard --config parse-dashboard-config.json",
    "ios-device-list": "instruments -s devices",
    "flow-start": "flow start",
    "flow-stop": "flow stop",
    "flow-status": "flow status",
    "flow-coverage": "flow coverage",
    "clean-android": "cd android && ./gradlew clean",
    "apk": "adb -d install --abi armeabi-v7a ./android/app/build/outputs/apk/release/app-armeabi-v7a-release.apk",
    "apk64": "adb -d install --abi arm64-v8a ./android/app/build/outputs/apk/release/app-arm64-v8a-release.apk",
    "apkold": "adb -d install ./android/app/build/outputs/apk/release/app-release.apk",
    "jsdoc": "./node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.4",
    "@react-native-community/viewpager": "^5.0.11",
    "@react-native-firebase/app": "^14.11.0",
    "@react-native-firebase/messaging": "^14.11.0",
    "create-react-class": "^15.7.0",
    "deprecated-react-native-listview": "0.0.7",
    "docdash": "^1.2.0",
    "jsdoc": "^3.6.7",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "17.0.2",
