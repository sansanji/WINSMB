    "react-native-voice": "^0.3.0",
    "react-native-webview": "^11.13.0",
    "react-redux": "^7.2.5",
    "react-timer-mixin": "^0.13.4",
    "redux": "^4.1.1",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0",
    "rn-material-ui-textfield": "^1.0.5"
  },
