    "react-native-a-beep": "^1.1.0",
    "react-native-background-timer": "^2.4.1",
    "react-native-barcode-svg": "0.0.15",
    "react-native-beacons-manager": "^1.0.7",
    "react-native-button": "^3.0.1",
